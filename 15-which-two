public String whichTwo(Integer a, Integer b, Integer c) {
    //code here
    if(a == null || b == null || c == null){
        return '';
    }
    if(a + b == c){
        return 'c';
    }else if(b + c == a){
        return 'a';
    }else if(c + a == b){
        return 'b';
    }else{
        return '';
    }
}